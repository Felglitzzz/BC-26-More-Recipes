{
  "name": "more-recipes",
  "version": "1.0.0",
  "private": true,
  "description": "A Recipe management and sharing app",
  "main": "/server/app.js",
  "scripts": {
    "start": "concurrently \"npm run prod:client\" \"npm run prod:server\"",
    "start:dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:server": "nodemon --watch server --exec babel-node -- server/app.js",
    "dev:client": "webpack --config webpack.dev.js --watch",
    "prod:server": "babel-node server/app.js",
    "prod:client": "webpack --config webpack.prod.js",
    "test": "mocha --timeout 500000 --compilers js:babel-core/register tests/",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Lawal Lanre Emmanuel",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
