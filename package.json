    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "cloudinary": "^1.9.1",
    "compression-webpack-plugin": "^1.1.2",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "jsonwebtoken": "^7.4.3",
    "mocha": "^3.4.2",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "net": "^1.0.2",
    "node-sass": "^4.6.1",
    "nodemailer": "^4.1.0",
    "nodemon": "^1.12.1",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
